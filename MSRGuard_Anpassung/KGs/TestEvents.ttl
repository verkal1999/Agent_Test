dp:hasVariableScope a owl:DatatypeProperty ;
    rdfs:range [ a rdfs:Datatype ;
            owl:oneOf [ a rdf:List ;
                    rdf:first "global" ;
                    rdf:rest [ a rdf:List ;
                            rdf:first "input" ;
                            rdf:rest [ a rdf:List ;
                                    rdf:first "local" ;
                                    rdf:rest [ a rdf:List ;
                                            rdf:first "output" ;
                                            rdf:rest () ] ] ] ] ] ;
    rdfs:subPropertyOf dp:hasVariableAttribute .

