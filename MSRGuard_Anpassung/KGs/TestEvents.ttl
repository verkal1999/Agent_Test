    GVL.Weiter := TRUE;
    Busy := FALSE;
    OPCUA.TriggerD2 := FALSE;
	OPCUA.bool2 := FALSE;
    GVL.Fehler := FALSE;
    Diagnose_gefordert := FALSE;
	GVL.DiagnoseRequested := FALSE;
END_IF
IF rtD3.Q THEN
	Alt_gefunden := TRUE;
END_IF""" ;
