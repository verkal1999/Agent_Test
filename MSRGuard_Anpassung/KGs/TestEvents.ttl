	END_IF
	IF NOT Stoerung_erkannt THEN
		Schritt4 := TRUE;
		OPCUA.lastExecutedSkill := 'TestSkill4';
	END_IF
END_IF

IF tSchritt4.Q THEN
	Schritt4 := FALSE;
	Schritt3 := TRUE;
	OPCUA.lastExecutedSkill := 'TestSkill3';
END_IF""" ;
