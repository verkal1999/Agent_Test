        ag:Port_FB_Automatikbetrieb_F1_PeriodicFaultEnable,
        ag:Port_FB_Automatikbetrieb_F1_PeriodicFaultPeriod,
        ag:Port_FB_Automatikbetrieb_F1_Stoerung_erkannt ;
